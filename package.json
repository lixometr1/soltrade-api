    "canvas": "^2.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudscraper": "^4.6.0",
    "crypto": "^1.0.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "fetch-cookie": "^1.0.0",
    "form-data": "^4.0.0",
    "https-proxy-agent": "^5.0.0",
    "instagram-private-api": "^1.45.2",
    "instagram-web-api": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.13.13",
    "node-fetch": "^2.6.6",
    "query-string": "^7.0.1",
    "random-useragent": "^0.5.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "socket.io": "^4.4.1",
